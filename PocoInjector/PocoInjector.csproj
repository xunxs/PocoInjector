<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProjectGuid>{12345678-1234-5678-9ABC-123456789012}</ProjectGuid>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>PocoInjector</RootNamespace>
    <AssemblyName>PocoInjector</AssemblyName>
    <TargetFrameworkVersion>v4.8</TargetFrameworkVersion>
    <FileAlignment>512</FileAlignment>
    <Deterministic>true</Deterministic>
    <!-- 游戏路径配置 - 请根据您的实际游戏路径修改这些变量 -->
    <GameRootPath Condition="'$(GameRootPath)' == ''">D:\Program Files (x86)\Steam\steamapps\common\Astral Party</GameRootPath>
    <GameName Condition="'$(GameName)' == ''">Astral Party</GameName>
    <PocoSDKPath Condition="'$(PocoSDKPath)' == ''">D:\workspace\Poco-SDK-master\Unity3D</PocoSDKPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug\</OutputPath>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release\</OutputPath>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xml" />
    
    <!-- BepInEx 核心库 - 使用游戏目录中的 BepInEx -->
    <Reference Include="BepInEx.Core">
      <HintPath>$(GameRootPath)\BepInEx\core\BepInEx.Core.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="BepInEx.Unity.IL2CPP">
      <HintPath>$(GameRootPath)\BepInEx\core\BepInEx.Unity.IL2CPP.dll</HintPath>
      <Private>False</Private>
    </Reference>
    
    <!-- Unity 核心库 - 使用游戏中的 Unity 引擎文件 -->
    <Reference Include="UnityEngine.CoreModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.CoreModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.UI">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.UI.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.UIElementsModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.UIElementsModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.IMGUIModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.IMGUIModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.UIModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.UIModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.TextRenderingModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.TextRenderingModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.TextCoreTextEngineModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.TextCoreTextEngineModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="UnityEngine.InputLegacyModule">
      <HintPath>$(GameRootPath)\BepInEx\interop\UnityEngine.InputLegacyModule.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="Unity.TextMeshPro">
      <HintPath>$(GameRootPath)\BepInEx\interop\Unity.TextMeshPro.dll</HintPath>
      <Private>False</Private>
    </Reference>
    
    <!-- IL2CPP 核心库 -->
    <Reference Include="Il2Cppmscorlib">
      <HintPath>$(GameRootPath)\BepInEx\interop\Il2Cppmscorlib.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="Il2CppInterop.Runtime">
      <HintPath>$(GameRootPath)\BepInEx\core\Il2CppInterop.Runtime.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="Il2CppInterop.Common">
      <HintPath>$(GameRootPath)\BepInEx\core\Il2CppInterop.Common.dll</HintPath>
      <Private>False</Private>
    </Reference>
    
    <!-- Poco SDK Unity3D 文件 - 需要编译为 DLL 或直接引用源码 -->
    <!-- 注意：Unity3D 版本的 Poco SDK 是 C# 源码，需要编译或直接引用 -->
    
    <!-- Newtonsoft.Json 依赖 -->
    <Reference Include="Newtonsoft.Json">
      <HintPath>$(PocoSDKPath)\3rdLib\Newtonsoft.Json.dll</HintPath>
      <Private>True</Private>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="Injector.cs" />
    <Compile Include="UiHighlighter.cs" />
    <Compile Include="UiAnnotator.cs" />
    <Compile Include="UiOverlayCamera.cs" />
    <!-- Poco SDK Unity3D 源码文件 - 暂时注释掉以避免 IL2CPP 兼容性问题 -->
    <!--
    <Compile Include="PocoManager.cs" />
    <Compile Include="UnityDumper.cs" />
    <Compile Include="AbstractDumper.cs" />
    <Compile Include="AbstractNode.cs" />
    <Compile Include="UnityNode.cs" />
    <Compile Include="ConcurrentDictionary.cs" />
    <Compile Include="PocoListenersBase.cs" />
    <Compile Include="PocoListenerUtils.cs" />
    <Compile Include="PocoMethodAttribute.cs" />
    <Compile Include="TcpClientConnectedEventArgs.cs" />
    <Compile Include="TcpClientDisconnectedEventArgs.cs" />
    <Compile Include="TcpClientState.cs" />
    <Compile Include="TcpDatagramReceivedEventArgs.cs" />
    <Compile Include="TcpServer.cs" />
    <Compile Include="VRSupport.cs" />
    -->
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>
